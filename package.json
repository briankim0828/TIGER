{
  "name": "pr",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "db:reset:sqlite": "tsx scripts/resetLocalSqlite.ts",
    "db:seed:remote:catalog": "echo 'Run this with psql: psql $DATABASE_URL -f sql/seed/exercise_catalog_public_seed.sql'",
    "db:apply:baseline": "psql $DATABASE_URL -f drizzle/sync/0000_cp6_baseline.sql",
    "db:apply:baseline:node": "tsx scripts/applyBaseline.ts",
    "db:verify:tables": "psql $DATABASE_URL -c \"\\dt\"",
    "db:verify:constraints": "psql $DATABASE_URL -c \"SELECT conname FROM pg_constraint WHERE conname ILIKE '%workout_sets%'\"",
    "db:seed:catalog": "psql $DATABASE_URL -f sql/seed/exercise_catalog_public_seed.sql",
    "db:rls:enable": "psql $DATABASE_URL -f sql/rls/mvp_enable_rls.sql",
    "db:rls:policies": "psql $DATABASE_URL -f sql/rls/mvp_policies.sql",
    "validate": "npm run -s typecheck",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@electric-sql/react": "^1.0.7",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-style/react": "^1.0.57",
    "@gluestack-ui/config": "^1.1.20",
    "@gluestack-ui/themed": "^1.1.73",
  "@gorhom/bottom-sheet": "^5.0.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/material-top-tabs": "^7.3.4",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.49.4",
    "base64-arraybuffer": "^1.0.2",
    "drizzle-orm": "^0.44.4",
    "electric-sql": "^0.12.1",
    "expo": "^54.0.18",
    "expo-constants": "~18.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "native-base": "^3.4.28",
    "pg": "^8.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-pager-view": "6.9.1",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "recyclerlistview": "^4.2.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/cli": "^18.0.0",
    "@types/pg": "^8.15.5",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tsx": "^4.20.4",
    "typescript": "~5.9.2"
